{
  "manifest_version": 2,
  "version": "1.2.3",
  "name": "Plato Enhancement Suite",
  "short_name": "PES",
  "description": "Plato Enhancement Suite provides useful utilites, functionality, and style changes to the existing Plato website.",
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["https://learn.pct.edu/*"],
      "js": ["jquery.min.js","main.br.min.js"]
    }
  ],
  "browser_action": {
    "default_icon": "img/icons/icon16.png",
    "default_popup": "settings.html"
  },
  "icons":{
    "16":"img/icons/icon16.png",
    "48":"img/icons/icon48.png",
    "128":"img/icons/icon128.png"
  }
}